<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/linear_listitem_apply_job"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="100"
            android:paddingBottom="5dp"
            android:paddingTop="5dp"
            android:background="@xml/list_item_selector">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="100">
                <TextView
                    android:id="@+id/tv_experiance_position"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="17dp"
                    android:textColor="@color/font_heading2_color" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingBottom="11dp">
                    <!--<LinearLayout-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:orientation="horizontal"-->
                    <!--android:weightSum="100">-->
                    <TextView
                        android:id="@+id/tv_cpy_nm_experiance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Google inc."
                        android:textSize="@dimen/textSize"
                        android:textColor="@color/font_normal"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentStart="true" />

                    <!--</LinearLayout>-->
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:weightSum="100">
                    <TextView
                        android:id="@+id/tv_st_dt_education_listitem"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="60"
                        android:text="septmer 2018 - june 2018"
                        android:textSize="@dimen/textSize"
                        android:textColor="@color/font_normal" />
                    <TextView
                        android:id="@+id/tv_location_experiance_listitem"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:layout_weight="40"
                        android:text="vancouver,bc"
                        android:textSize="@dimen/textSize"
                        android:textColor="@color/my_sky_dark" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
    <TextView
        android:id="@+id/tv_expiriance_detials_listitem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="right"
        android:layout_marginLeft="10dp"
        android:text=""
        android:textSize="@dimen/textSize"
        android:textColor="#5D85AF" />
    <LinearLayout
        android:id="@+id/linear_line_experiance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:gravity="bottom">
        <View
            android:gravity="bottom"
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:background="@color/my_orange"/>
    </LinearLayout>
</LinearLayout>
